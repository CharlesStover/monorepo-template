{
  "name": "@monorepo-template/monorepo-template",
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "react-application:build": "yarn workspace @monorepo-template/react-application run build",
    "react-application:coverage": "yarn workspace @monorepo-template/react-application run coverage",
    "react-application:cypress:run": "yarn workspace @monorepo-template/react-application run cypress:run",
    "react-application:cypress:start": "yarn workspace @monorepo-template/react-application run cypress:start",
    "react-application:eslint": "yarn workspace @monorepo-template/react-application run eslint",
    "react-application:eslint:fix": "yarn workspace @monorepo-template/react-application run eslint:fix",
    "react-application:jest": "yarn workspace @monorepo-template/react-application run jest",
    "react-application:jest:watch": "yarn workspace @monorepo-template/react-application run jest:watch",
    "react-application:start": "yarn workspace @monorepo-template/react-application run start",
    "eslint": "eslint '**/*' --ignore-pattern 'packages/**/*'",
    "react-module:eslint": "yarn workspace @monorepo-template/react-module run eslint",
    "react-module:eslint:fix": "yarn workspace @monorepo-template/react-module run eslint:fix",
    "react-module:jest": "yarn workspace @monorepo-template/react-module run jest",
    "react-module:jest:watch": "yarn workspace @monorepo-template/react-module run jest:watch",
    "react-module:rollup": "yarn workspace @monorepo-template/react-module run rollup",
    "react-module:rollup:watch": "yarn workspace @monorepo-template/react-module run rollup:watch",
    "react-module:start": "yarn workspace @monorepo-template/react-module run start",
    "test": "monorepo-template-test"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@monorepo-template/eslint-config-typescript": "^0.1.4",
    "@monorepo-template/eslint-config-typescript-fixable": "^0.2.2",
    "@monorepo-template/eslint-config-typescript-strict": "^0.1.0",
    "@monorepo-template/test": "^1.0.3",
    "@monorepo-template/tsconfig": "^0.1.1",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@yarnpkg/sdks": "^2.6.0-rc.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}
